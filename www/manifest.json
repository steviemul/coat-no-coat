{
  "name": "Coat / No Coat",
  "description": "Coat / No Coat Chrome extension",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "permissions": [
    "geolocation",
    "<all_urls>",
    "background",
    "tabs",
    "notifications"
  ],
  "background" : {
    "page":"background.html"
  },
  "icons": {
    "72": "images/cartman72x72.png"
  },
  "browser_action": {
    "default_icon": {
      "42": "images/cartman42x42.png"
    },
    "default_title": "Coat / No Coat"
  }
}